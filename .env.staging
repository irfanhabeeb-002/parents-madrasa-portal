# Staging Environment Configuration
NODE_ENV=staging
VITE_APP_ENV=staging
VITE_APP_VERSION=1.0.0

# Firebase Configuration (Staging)
# Replace with actual staging Firebase project credentials
VITE_FIREBASE_API_KEY=your_staging_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-staging-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-staging-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-staging-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_staging_sender_id
VITE_FIREBASE_APP_ID=your_staging_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_staging_measurement_id

# Zoom Configuration (Staging)
VITE_ZOOM_ENABLED=true
VITE_ZOOM_API_KEY=your_staging_zoom_api_key
VITE_ZOOM_API_SECRET=your_staging_zoom_api_secret
VITE_ZOOM_ACCOUNT_ID=your_staging_zoom_account_id
VITE_ZOOM_CLIENT_ID=your_staging_zoom_client_id
VITE_ZOOM_CLIENT_SECRET=your_staging_zoom_client_secret
VITE_ZOOM_REDIRECT_URL=https://staging.yourapp.com/auth/zoom/callback

# Analytics Configuration (Staging)
VITE_GA_MEASUREMENT_ID=G-STAGING-XXXX
VITE_ANALYTICS_ENABLED=true

# Security Configuration (Staging)
VITE_ENABLE_CONSOLE_LOGS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id

# Performance Monitoring (Staging)
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLE_RATE=1.0

# Feature Flags (Staging)
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_WHATSAPP_INTEGRATION=true

# API Configuration (Staging)
VITE_API_BASE_URL=https://staging-api.yourapp.com
VITE_API_TIMEOUT=15000

# WhatsApp Integration (Staging)
VITE_WHATSAPP_TEACHER_NUMBER=+919876543210
VITE_WHATSAPP_ENABLED=true