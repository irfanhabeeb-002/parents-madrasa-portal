# Environment Configuration
NODE_ENV=development
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Zoom Configuration
# Set to "true" to enable Zoom integration, "false" to disable
VITE_ZOOM_ENABLED=false

# Get these from your Zoom Marketplace App (only needed when VITE_ZOOM_ENABLED=true)
VITE_ZOOM_API_KEY=your_zoom_api_key_here
VITE_ZOOM_API_SECRET=your_zoom_api_secret_here
VITE_ZOOM_ACCOUNT_ID=your_zoom_account_id_here
VITE_ZOOM_CLIENT_ID=your_zoom_client_id_here
VITE_ZOOM_CLIENT_SECRET=your_zoom_client_secret_here
VITE_ZOOM_REDIRECT_URL=http://localhost:5173/auth/zoom/callback

# Analytics Configuration
VITE_GA_MEASUREMENT_ID=your_ga_measurement_id_here
VITE_ANALYTICS_ENABLED=false

# Security Configuration
VITE_ENABLE_CONSOLE_LOGS=true
VITE_ENABLE_ERROR_REPORTING=false
VITE_SENTRY_DSN=your_sentry_dsn_here

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_PERFORMANCE_SAMPLE_RATE=0.1

# Feature Flags
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_WHATSAPP_INTEGRATION=true

# API Configuration
VITE_API_BASE_URL=https://api.yourproject.com
VITE_API_TIMEOUT=10000

# WhatsApp Integration
VITE_WHATSAPP_TEACHER_NUMBER=+919876543210
VITE_WHATSAPP_ENABLED=true