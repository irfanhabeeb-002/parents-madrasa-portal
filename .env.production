# Production Environment Configuration
NODE_ENV=production
VITE_APP_ENV=production
VITE_APP_VERSION=1.0.0

# Firebase Configuration (Production)
# Replace with actual production Firebase project credentials
VITE_FIREBASE_API_KEY=your_production_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-production-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-production-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-production-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_production_sender_id
VITE_FIREBASE_APP_ID=your_production_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_production_measurement_id

# Zoom Configuration (Production)
VITE_ZOOM_ENABLED=true
VITE_ZOOM_API_KEY=your_production_zoom_api_key
VITE_ZOOM_API_SECRET=your_production_zoom_api_secret
VITE_ZOOM_ACCOUNT_ID=your_production_zoom_account_id
VITE_ZOOM_CLIENT_ID=your_production_zoom_client_id
VITE_ZOOM_CLIENT_SECRET=your_production_zoom_client_secret
VITE_ZOOM_REDIRECT_URL=https://yourapp.com/auth/zoom/callback

# Analytics Configuration (Production)
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_ANALYTICS_ENABLED=true

# Security Configuration (Production)
VITE_ENABLE_CONSOLE_LOGS=false
VITE_ENABLE_ERROR_REPORTING=true
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Performance Monitoring (Production)
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLE_RATE=0.1

# Feature Flags (Production)
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_WHATSAPP_INTEGRATION=true

# API Configuration (Production)
VITE_API_BASE_URL=https://api.yourproductionapp.com
VITE_API_TIMEOUT=10000

# WhatsApp Integration (Production)
VITE_WHATSAPP_TEACHER_NUMBER=+919876543210
VITE_WHATSAPP_ENABLED=true